{
	"name": "svelte-kit-starter",
	"version": "0.0.1",
	"private": true,
	"prisma": {
		"seed": "npx vite-node prisma/seed.ts"
	},
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n prisma:studio",
		"vite": "vite dev",
		"typesafe-i18n": "typesafe-i18n",
		"prisma:studio": "npx prisma studio --browser none",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.2.1",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.4",
		"prisma": "^4.11.0",
		"prisma-erd-generator-markdown": "^1.3.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typesafe-i18n": "^5.24.2",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^0.5.3",
		"@lucia-auth/sveltekit": "^0.6.9",
		"@prisma/client": "^4.11.0",
		"daisyui": "^2.51.3",
		"lucia-auth": "^0.8.0",
		"svelte-french-toast": "^1.0.3",
		"zod": "^3.21.4"
	}
}
